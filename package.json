  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anxpara/projectrix/issues"
  },
  "homepage": "https://github.com/anxpara/projectrix#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "pm2": "^5.2.0",
    "rollup": "^3.7.4",
    "rollup-plugin-dts": "^5.0.0",
    "sass": "^1.54.0",
    "terser": "^5.14.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  }
